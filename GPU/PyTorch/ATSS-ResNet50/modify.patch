@@ -0,0 +1,46 @@
+import os, sys
+import argparse
+from importlib import import_module
+
+
+if __name__ == '__main__':
+    
+    parser = argparse.ArgumentParser()
+    parser.add_argument('--config', type=str, default='./configs/atss/atss_r50_fpn_1x_coco.py')
+    parser.add_argument('--config_base', type=list, default=["../_base_/datasets/coco_detection.py","../_base_/schedules/schedule_1x.py"])
+    parser.add_argument('--data_path', type=str, default="/algo/modelzoo/datasets/COCO17/")
+    parser.add_argument('--batch_size', type=int, default=2)
+    parser.add_argument('--epochs', type=int, default=12)
+    args = parser.parse_args()
+
+    config_path, config_name = os.path.split(args.config)
+    sys.path.append(config_path)
+    config_base = import_module(os.path.splitext(config_name)[0])._base_
+
+    for config_file in config_base:
+        if config_file in args.config_base:
+            module_path, module_name = os.path.split(config_file)
+
+            sys.path.append(os.path.join(config_path, module_path))
+            config = import_module(os.path.splitext(module_name)[0])
+
+            cont = open(os.path.join(config_path, config_file)).readlines()
+            for idx in range(len(cont)):
+                try:
+                    if config.data_root in cont[idx]:
+                        cont[idx] = cont[idx].replace(config.data_root, args.data_path)
+                except:
+                    pass
+                try:
+                    if "samples_per_gpu" in cont[idx]:
+                        cont[idx] = cont[idx].replace(str(config.data["samples_per_gpu"]), str(args.batch_size))
+                except:
+                    pass
+                try:
+                    if "max_epochs" in cont[idx]:
+                        cont[idx] = cont[idx].replace(str(config.runner["max_epochs"]), str(args.epochs))
+                except:
+                    pass
+
+            f = open(os.path.join(config_path, config_file), "w")
+            for line in cont: f.write(line)
